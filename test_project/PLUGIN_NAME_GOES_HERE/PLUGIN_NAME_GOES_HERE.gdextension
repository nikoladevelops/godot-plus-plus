[configuration]
entry_symbol = "plugin_name_goes_here_init"
compatibility_minimum = "4.4"

[libraries]
; Relative paths ensure that our GDExtension can be placed anywhere in the project directory.
macos.debug = "./bin/macos/libplugin_name_goes_here.macos.template_debug.framework"
macos.release = "./bin/macos/libplugin_name_goes_here.macos.template_release.framework"

ios.debug = "./bin/ios/libplugin_name_goes_here.ios.template_debug.xcframework"
ios.release = "./bin/ios/libplugin_name_goes_here.ios.template_release.xcframework"

windows.x86_32.debug = "./bin/windows/plugin_name_goes_here.windows.template_debug.x86_32.dll"
windows.x86_32.release = "./bin/windows/plugin_name_goes_here.windows.template_release.x86_32.dll"
windows.x86_64.debug = "./bin/windows/plugin_name_goes_here.windows.template_debug.x86_64.dll"
windows.x86_64.release = "./bin/windows/plugin_name_goes_here.windows.template_release.x86_64.dll"
windows.arm64.debug = "./bin/windows/plugin_name_goes_here.windows.template_debug.arm64.dll"
windows.arm64.release = "./bin/windows/plugin_name_goes_here.windows.template_release.arm64.dll"

linux.x86_32.debug = "./bin/linux/libplugin_name_goes_here.linux.template_debug.x86_32.so"
linux.x86_32.release = "./bin/linux/libplugin_name_goes_here.linux.template_release.x86_32.so"
linux.x86_64.debug = "./bin/linux/libplugin_name_goes_here.linux.template_debug.x86_64.so"
linux.x86_64.release = "./bin/linux/libplugin_name_goes_here.linux.template_release.x86_64.so"
linux.arm32.debug = "./bin/linux/libplugin_name_goes_here.linux.template_debug.arm32.so"
linux.arm32.release = "./bin/linux/libplugin_name_goes_here.linux.template_release.arm32.so"
linux.arm64.debug = "./bin/linux/libplugin_name_goes_here.linux.template_debug.arm64.so"
linux.arm64.release = "./bin/linux/libplugin_name_goes_here.linux.template_release.arm64.so"
linux.rv64.debug = "./bin/linux/libplugin_name_goes_here.linux.template_debug.rv64.so"
linux.rv64.release = "./bin/linux/libplugin_name_goes_here.linux.template_release.rv64.so"

android.x86_64.debug = "./bin/android/libplugin_name_goes_here.android.template_debug.x86_64.so"
android.x86_64.release = "./bin/android/libplugin_name_goes_here.android.template_release.x86_64.so"
android.arm64.debug = "./bin/android/libplugin_name_goes_here.android.template_debug.arm64.so"
android.arm64.release = "./bin/android/libplugin_name_goes_here.android.template_release.arm64.so"
android.arm32.debug = "./bin/android/libplugin_name_goes_here.android.template_debug.arm32.so"
android.arm32.release = "./bin/android/libplugin_name_goes_here.android.template_release.arm32.so"

web.debug.threads.wasm32 = "./bin/web/libplugin_name_goes_here.web.template_debug.wasm32.threads.wasm"
web.release.threads.wasm32 = "./bin/web/libplugin_name_goes_here.web.template_release.wasm32.threads.wasm"
web.debug.wasm32 = "./bin/web/libplugin_name_goes_here.web.template_debug.wasm32.nothreads.wasm"
web.release.wasm32 = "./bin/web/libplugin_name_goes_here.web.template_release.wasm32.nothreads.wasm"

[dependencies]
macos.debug = { "./bin/macos/libgodot-cpp.macos.template_debug.framework": "" }
macos.release = { "./bin/macos/libgodot-cpp.macos.template_release.framework": "" }
ios.debug = { "./bin/ios/libgodot-cpp.ios.template_debug.xcframework": "" }
ios.release = { "./bin/ios/libgodot-cpp.ios.template_release.xcframework": "" }